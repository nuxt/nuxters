{
  "name": "nuxters",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "logs": "wrangler pages deployment tail",
    "test:browser": "playwright test test/browser",
    "test:browser:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-node-modules:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.55.1-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/browser --update-snapshots'",
    "test:nuxt": "vitest --project nuxt",
    "test:types": "vue-tsc -b --noEmit",
    "test:unit": "vitest --project unit"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.20.1",
    "@playwright/test": "^1.56.1",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4",
    "vue-tsc": "^3.1.2",
    "wrangler": "^4.45.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.55",
    "@nuxt/devtools": "^2.7.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/plausible": "^2.0.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "nuxt": "^4.2.0",
    "nuxt-og-image": "^5.1.12",
    "octokit": "^5.0.4",
    "ofetch": "^1.4.1",
    "tailwindcss": "^4.1.16",
    "ufo": "^1.6.1",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.17.1",
    "vue": "^3.5.22",
    "vue-confetti-explosion": "^1.0.2"
  },
  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}

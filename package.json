{
  "name": "nuxters",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "logs": "wrangler pages deployment tail",
    "test:browser": "playwright test test/browser",
    "test:browser:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-node-modules:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.55.1-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/browser --update-snapshots'",
    "test:nuxt": "vitest --project nuxt",
    "test:types": "vue-tsc -b --noEmit",
    "test:unit": "vitest --project unit"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/plausible": "^2.0.1",
    "@playwright/test": "^1.55.1",
    "eslint": "^9.36.0",
    "happy-dom": "^19.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0",
    "wrangler": "^4.40.2"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.53",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "4.0.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/plausible": "^2.0.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "nuxt": "^4.1.2",
    "nuxt-og-image": "^5.1.11",
    "octokit": "^5.0.3",
    "ofetch": "^1.4.1",
    "tailwindcss": "^4.1.13",
    "ufo": "^1.6.1",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.17.1",
    "vue": "^3.5.22",
    "vue-confetti-explosion": "^1.0.2"
  },
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}

{
  "name": "nuxters",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "lint": "eslint .",
    "collect:contributors": "tsx scripts/collect-contributors.ts",
    "postinstall": "nuxt prepare",
    "logs": "wrangler pages deployment tail",
    "test:browser": "playwright test test/browser",
    "test:browser:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-node-modules:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.55.1-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/browser --update-snapshots'",
    "test:nuxt": "vitest --project nuxt",
    "test:types": "vue-tsc -b --noEmit",
    "test:unit": "vitest --project unit"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/test-utils": "^3.21.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "happy-dom": "^20.0.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1",
    "wrangler": "^4.56.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.64",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.3.0",
    "@nuxthub/core": "^0.10.4",
    "@nuxtjs/plausible": "^2.0.1",
    "@octokit/plugin-paginate-rest": "^14.0.0",
    "@octokit/plugin-retry": "^8.0.3",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.3.1",
    "nuxt-og-image": "^5.1.13",
    "octokit": "^5.0.5",
    "ofetch": "^1.5.1",
    "tailwindcss": "^4.1.18",
    "ufo": "^1.6.1",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.17.3",
    "vue": "^3.5.26",
    "vue-confetti-explosion": "^1.0.2"
  },
  "packageManager": "pnpm@10.27.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
